<Window x:Class="FITAPP.AddDiet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FITAPP"
        mc:Ignorable="d"
        Title="Utwórz Dietę" Height="600" Width="800" Closing="Window_Closing">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="40*"/>
        </Grid.RowDefinitions>
        <ProgressBar x:Name="pasekPostepu" Grid.Row="0" Grid.Column="0"/>
        <Grid x:Name="grid" Grid.Row="1" Grid.Column="0"/>
    </Grid>
</Window>
